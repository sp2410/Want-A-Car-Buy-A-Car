<% if user_signed_in? %> 
    <% if (current_user.role.eql? "premium") or (current_user.role.eql? "dealer") %>
          <% if @wholesalelistings.present? %>
            <hr />
            <div class="center-column" id="right-center-column2" >         
            <h1>WHOLESALE LISTINGS</h1>       
              <% @wholesalelistings.each do |wlisting| %>              
                  <div id = "listingsdescsmall">                
                      <%= link_to wlisting do %>
                          <p style="margin-top: -10px;">
                              
                              <% if wlisting.image_url.present? %> 
                                <%= image_tag wlisting.image.url(:thumb) if wlisting.image_url.present? %> 
                                <br>
                              <% else %>
                                <%= image_tag("https://s3-us-west-2.amazonaws.com/wacbacassetsdonttouch/wacbacassets/nopictures.png", :alt => "want a car buy a car") %>
                              <% end %>
                            
                                <%= wlisting.city %><br> <%= wlisting.state %><br> Price: <%= wlisting.price %>                                    
                          </p>
                      <% end %>
                  </div>                   
              <% end %>
              </div> 
          <% end %>       
    <% end %>  
<% end %>